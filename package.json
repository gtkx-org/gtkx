{
    "name": "gtkx",
    "version": "0.0.0",
    "private": true,
    "homepage": "https://eugeniodepalo.github.io/gtkx/",
    "license": "MPL-2.0",
    "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
    "type": "module",
    "scripts": {
        "build": "turbo build --filter=!website --filter=!gtkx",
        "codegen": "turbo codegen:run",
        "codegen:sync": "gtkx-codegen sync",
        "codegen:run": "gtkx-codegen run",
        "docs": "turbo build --filter=website",
        "knip": "turbo knip:all",
        "knip:all": "knip --no-gitignore --no-config-hints",
        "lint": "turbo lint:all && turbo lint --filter=!website --filter=!gtkx",
        "lint:all": "biome check .",
        "test": "turbo test --filter=!website --filter=!gtkx"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.11",
        "@gtkx/codegen": "workspace:*",
        "@types/node": "^25.0.3",
        "@types/react": "^19.2.7",
        "knip": "^5.80.1",
        "react": "^19.2.3",
        "turbo": "^2.7.3",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16"
    },
    "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
