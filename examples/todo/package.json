{
    "name": "todo",
    "version": "0.0.0",
    "private": true,
    "description": "Todo app demonstrating GTKX with Adwaita integration",
    "type": "module",
    "gtkx": {
        "appId": "com.gtkx.todo"
    },
    "scripts": {
        "build": "tsc -b",
        "dev": "gtkx dev src/dev.tsx",
        "start": "node dist/index.js",
        "test": "vitest run"
