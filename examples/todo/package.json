    "dependencies": {
        "@gtkx/ffi": "workspace:*",
        "@gtkx/react": "workspace:*",
        "react": "^19.2.3"
    },
    "devDependencies": {
        "@gtkx/cli": "workspace:*",
        "@gtkx/testing": "workspace:*",
        "@gtkx/vitest": "workspace:*"
    }
}
