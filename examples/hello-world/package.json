{
    "name": "hello-world",
    "version": "0.0.0",
    "private": true,
    "description": "Simple counter app demonstrating GTKX basics",
    "type": "module",
    "gtkx": {
        "appId": "com.gtkx.hello-world"
    },
    "scripts": {
        "build": "gtkx build",
        "typecheck": "tsc -b --noEmit",
        "dev": "gtkx dev src/dev.tsx",
        "start": "node dist/bundle.js"
    },
    "dependencies": {
        "@gtkx/ffi": "workspace:*",
        "@gtkx/react": "workspace:*",
        "react": "^19.2.4"
    },
    "devDependencies": {
        "@gtkx/cli": "workspace:*",
        "vite": "^7.3.1"
    }
}
