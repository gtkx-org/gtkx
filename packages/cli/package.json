{
    "name": "@gtkx/cli",
    "version": "0.18.9",
    "description": "CLI for GTKX - create and develop GTK4 React applications",
    "keywords": [
        "gtkx",
        "gtk",
        "gtk4",
        "react",
        "typescript",
        "native",
        "linux",
        "desktop",
        "cli",
        "scaffold",
        "hmr",
        "vite"
    ],
    "homepage": "https://gtkx.dev",
    "bugs": {
        "url": "https://github.com/gtkx-org/gtkx/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gtkx-org/gtkx.git",
        "directory": "packages/cli"
    },
    "license": "MPL-2.0",
    "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./builder": {
            "types": "./dist/builder.d.ts",
            "default": "./dist/builder.js"
        },
        "./dev-server": {
            "types": "./dist/dev-server.d.ts",
            "default": "./dist/dev-server.js"
        },
        "./refresh-runtime": {
            "types": "./dist/refresh-runtime.d.ts",
            "default": "./dist/refresh-runtime.js"
        }
    },
    "bin": {
        "gtkx": "bin/gtkx.js"
    },
    "sideEffects": false,
    "files": [
        "bin",
        "dist",
        "src",
        "templates"
    ],
    "scripts": {
        "build": "tsc -b",
        "test": "vitest run"
    },
    "dependencies": {
        "@clack/prompts": "^1.0.0",
        "@gtkx/ffi": "workspace:*",
        "@gtkx/mcp": "workspace:*",
        "@gtkx/react": "workspace:*",
        "@swc/core": "^1.15.11",
        "citty": "^0.2.0",
        "ejs": "^4.0.1",
        "react-refresh": "^0.18.0",
        "vite": "^7.3.1"
    },
    "devDependencies": {
        "@gtkx/testing": "workspace:*",
        "@types/ejs": "^3.1.5",
        "@types/react-refresh": "^0.14.7",
        "memfs": "^4.56.10"
    },
    "peerDependencies": {
        "@gtkx/testing": "workspace:*",
        "react": "^19"
    },
    "peerDependenciesMeta": {
        "@gtkx/testing": {
            "optional": true
        }
    }
}
