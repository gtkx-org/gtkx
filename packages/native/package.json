{
    "name": "@gtkx/native",
    "version": "0.15.0",
    "description": "Rust-based native module providing FFI bindings for GTKX",
    "keywords": [
        "gtkx",
        "gtk",
        "gtk4",
        "react",
        "native",
        "ffi",
        "rust",
        "neon",
        "linux",
        "desktop"
    ],
    "homepage": "https://eugeniodepalo.github.io/gtkx",
    "bugs": {
        "url": "https://github.com/eugeniodepalo/gtkx/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eugeniodepalo/gtkx.git",
        "directory": "packages/native"
    },
    "license": "MPL-2.0",
    "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b && cp ../../README.md .",
        "lint": "cargo fmt --check && cargo clippy -- -D warnings",
        "native-build": "node scripts/build.js",
        "native-test": "xvfb-run -a cargo test -- --test-threads=1",
        "test": "vitest run"
    },
    "optionalDependencies": {
        "@gtkx/native-linux-arm64": "workspace:*",
        "@gtkx/native-linux-x64": "workspace:*"
    },
    "devDependencies": {
        "@gtkx/vitest": "workspace:*",
        "@neon-rs/cli": "^0.1.82"
    }
}
